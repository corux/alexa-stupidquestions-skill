{
  "name": "alexa-stupidquestions-skill",
  "version": "1.0.0",
  "description": "Alexa Stupid Questions Skill",
  "private": true,
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "test": "npm run build:prod",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "build": "webpack --config webpack.config.ts --require ts-node/register",
    "build:dev": "npm run build -- --mode development --watch",
    "build:prod": "npm run lint && npm run build -- --mode production",
    "deploy:prod": "ask deploy --force --profile default --target lambda",
    "convert-assets": "rsvg-convert -w 108 -o assets/icon108.png assets/icon.svg && rsvg-convert -w 512 -o assets/icon512.png assets/icon.svg",
    "create-s3-bucket": "aws s3 ls s3://alexa-stupidquestions-skill > /dev/null 2>&1 || aws s3 mb s3://alexa-stupidquestions-skill --region eu-west-1",
    "deploy:assets": "aws s3 sync ./assets s3://alexa-stupidquestions-skill/ --acl public-read"
  },
  "devDependencies": {
    "@types/node": "10.14.1",
    "ask-cli": "1.7.2",
    "ask-sdk-core": "2.5.2",
    "ask-sdk-dynamodb-persistence-adapter": "2.5.2",
    "ask-sdk-model": "1.15.1",
    "class.extend": "0.9.2",
    "raw-loader": "2.0.0",
    "ts-loader": "6.0.1",
    "ts-node": "8.2.0",
    "tslint": "5.16.0",
    "typescript": "3.4.5",
    "underscore": "1.9.1",
    "webpack": "4.32.2",
    "webpack-command": "0.4.2",
    "weighted-random-selection": "1.0.0"
  }
}
